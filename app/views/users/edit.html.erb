<h1>Modify your account</h1>

<%= form_for @user do |f| %>

<p><%= f.label :username %>
<%= f.text_field :username %></p>

<p><%= f.label "first name" %>
<%= f.text_field :first_name %></p>

<p><%= f.label "last name" %>
<%= f.text_field :last_name %></p>

<p><%= f.label :email %>
<%= f.text_field :email %></p>

<p><%= f.label :password_digest %>
<%= f.password_field :password %></p>

<p><%= f.submit %></p>

<% end %>
