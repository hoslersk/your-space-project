<h1>Modify your account</h1>

<%= form_for @user do |f| %>

<%= f.label :username %>
<%= f.text_field :username %>

<%= f.label "first name" %>
<%= f.text_field :first_name %>

<%= f.label "last name" %>
<%= f.text_field :last_name %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password_digest %>
<%= f.text_field :password_digest %>

<%= f.submit %>

<% end %>
