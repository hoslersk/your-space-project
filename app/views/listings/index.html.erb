<h1>Listings Index</h1>
<ul>
  <% @listings.each do |listing| %>
    <li><%= link_to listing.venue.name, listing_path(listing) %>
      <ul>
        <li>Available Dates: <%= listing.available_start_date.strftime("%B %e, %Y") %> - <%= listing.available_end_date.strftime("%B %e, %Y") %></li>
        <li>Available Hours: <%= listing.available_end_time.strftime("%H:%M") %> - <%= listing.available_end_time.strftime("%H:%M") %></li>
      </ul>
    </li>
  <% end %>
</ul>

<%= link_to "Add a Listing", new_listing_path %>
