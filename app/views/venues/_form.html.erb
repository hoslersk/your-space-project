<%= form_for @venue, :html => { :multipart => true } do |f| %>

<p><%= f.label :name%>
<%= f.text_field :name%></p>

<p><%= f.label :street_address%>
<%= f.text_field :address %></p>

<p><%= f.label :city %>
<%= f.text_field :city %></p>

<p><%= f.label :zip_code %>
<%= f.text_field :zip_code %></p>

<p><%= f.label :description%>
<%= f.text_field :description %></p>


<%= f.fields_for :images do |images_form| %>
  <p>
  <%= f.label :image %>
  <%= images_form.file_field :image %>
  <%= f.label :image_description %>
  <%= images_form.text_field :description %>
  </p>
<% end %>

<%= f.hidden_field :host_id, value: current_user.id %>

<p><%= f.submit %></p>

<% end %>
