<%= form_for(@venue) do |f| %>

<p><%= f.label :name%>
<%= f.text_field :name%></p>

<p><%= f.label :address%>
<%= f.text_field :address %></p>

<p><%= f.label :description%>
<%= f.text_field :description %></p>

<p><%= f.label :host_id%>
<%= f.number_field :host_id %></p>

<p><%= f.submit %></p>

<% end %>
